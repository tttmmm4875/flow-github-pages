{
  "name": "vue3-swagger-verification",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "generate-types": "openapi-typescript docs/api.yaml -o src/types/api.d.ts",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm start"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/tsconfig": "^0.7.0",
    "openapi-typescript": "^7.8.0",
    "typescript": "^5.9.2",
    "vite": "^5.4.0",
    "vite-plugin-vue-devtools": "^7.0.0",
    "vue-tsc": "^3.0.5"
  }
}
